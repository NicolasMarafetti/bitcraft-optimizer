{
  "name": "bitcraft-optimizer",
  "version": "1.0.0",
  "description": "Application d'optimisation pour BitCraft",
  "main": "public/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/app.js",
    "server:dev": "nodemon server/app.js",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run server:dev\" \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "db:init": "tsx src/scripts/initDatabase.ts",
    "db:clean": "node src/scripts/cleanDatabase.js",
    "db:setup": "npm run prisma:generate && npm run prisma:push"
  },
  "keywords": ["bitcraft", "optimizer", "gaming"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "concurrently": "^8.2.0",
    "wait-on": "^7.2.0",
    "cross-env": "^7.0.3",
    "prisma": "^5.7.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "nodemon": "^3.0.0"
  }
}